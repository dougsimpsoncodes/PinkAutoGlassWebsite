  "boxShadow": {
    "none": "none",
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
    "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
    "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
    "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)",
    "brand": "0 4px 14px 0 rgba(232, 90, 139, 0.2)",
    "brandHover": "0 8px 25px 0 rgba(232, 90, 139, 0.3)"
  },
  "splitCinematic": {
    "hero": {
      "minHeight": "100vh",
      "minHeightMobile": "80vh",
      "overlayGradient": "linear-gradient(45deg, rgba(0,0,0,0.6) 0%, rgba(0,0,0,0.3) 50%, rgba(0,0,0,0.7) 100%)",
      "contentMaxWidth": "600px",
      "contentPadding": {
        "desktop": "4rem",
        "tablet": "3rem",
        "mobile": "2rem"
      },
      "titleAnimation": {
        "delay": "0.2s",
        "duration": "0.8s",
        "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "subtitleAnimation": {
        "delay": "0.5s",
        "duration": "0.6s",
        "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "ctaAnimation": {
        "delay": "0.8s",
        "duration": "0.4s",
        "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      }
    },
    "imageSection": {
      "backgroundAttachment": "fixed",
      "backgroundSize": "cover",
      "backgroundPosition": "center",
      "filter": "brightness(0.8) contrast(1.1)"
    },
    "textSection": {
      "backgroundColor": "#ffffff",
      "padding": {
        "desktop": "6rem 4rem",
        "tablet": "4rem 3rem",
        "mobile": "3rem 2rem"
      },
      "maxWidth": "800px",
      "margin": "0 auto"
    }
  },
  "components": {
    "button": {
      "primary": {
        "background": "linear-gradient(135deg, #f0536d 0%, #ee6aa3 60%, #d946ef 100%)",
        "color": "#ffffff",
        "padding": "0.75rem 2rem",
        "borderRadius": "0.5rem",
        "fontWeight": "600",
        "fontSize": "1rem",
        "lineHeight": "1.5",
        "boxShadow": "0 4px 14px 0 rgba(232, 90, 139, 0.3)",
        "transition": "all 0.2s cubic-bezier(0.25, 0.46, 0.45, 0.94)",
        "hover": {
          "transform": "translateY(-2px)",
          "boxShadow": "0 8px 25px 0 rgba(232, 90, 139, 0.4)"
        },
        "focus": {
          "outline": "2px solid #E85A8B",
          "outlineOffset": "2px"
        }
      },
      "secondary": {
        "background": "transparent",
        "color": "#0B1830",
        "border": "2px solid #e5e7eb",
        "padding": "0.75rem 2rem",
        "borderRadius": "0.5rem",
        "fontWeight": "600",
        "fontSize": "1rem",
        "lineHeight": "1.5",
        "transition": "all 0.2s ease",
        "hover": {
          "borderColor": "#E85A8B",
          "color": "#E85A8B"
        },
        "focus": {
          "outline": "2px solid #E85A8B",
          "outlineOffset": "2px"
        }
      }
    },
    "badge": {
      "base": {
        "display": "inline-flex",
        "alignItems": "center",
        "gap": "0.5rem",
        "padding": "0.5rem 1rem",
        "borderRadius": "9999px",
        "fontSize": "0.875rem",
        "fontWeight": "500",
        "lineHeight": "1.25"
      },
      "primary": {
        "background": "linear-gradient(135deg, #E85A8B 0%, #ee6aa3 100%)",
        "color": "#ffffff"
      },
      "outline": {
        "background": "transparent",
        "color": "#E85A8B",
        "border": "1px solid #E85A8B"
      }
    },
    "card": {
      "base": {
        "backgroundColor": "#ffffff",
        "borderRadius": "1rem",
        "boxShadow": "var(--shadow-lg)",
        "padding": "2rem",
        "transition": "all 0.2s ease"
      },
      "quote": {
        "position": "relative",
        "backgroundColor": "#ffffff",
        "borderRadius": "1rem",
        "padding": "2.5rem 2rem 2rem",
        "boxShadow": "var(--shadow-lg)",
        "border": "1px solid #f3f4f6",
        "maxWidth": "500px"
      },
      "service": {
        "backgroundColor": "#ffffff",
        "borderRadius": "0.75rem",
        "padding": "1.5rem",
        "boxShadow": "var(--shadow-base)",
        "border": "1px solid #f3f4f6",
        "transition": "all 0.2s ease",
        "hover": {
          "transform": "translateY(-4px)",
          "boxShadow": "var(--shadow-xl)"
        }
      }
    },
    "chip": {
      "base": {
        "display": "inline-flex",
        "alignItems": "center",
        "padding": "0.375rem 0.75rem",
        "borderRadius": "9999px",
        "fontSize": "0.875rem",
        "fontWeight": "500",
        "transition": "all 0.2s ease"
      },
      "area": {
        "backgroundColor": "#f3f4f6",
        "color": "#374151",
        "hover": {
          "backgroundColor": "#e5e7eb",
          "transform": "scale(1.05)"
        }
      }
    },
    "accordion": {
      "container": {
        "backgroundColor": "#ffffff",
        "borderRadius": "0.5rem",
        "boxShadow": "var(--shadow-base)",
        "overflow": "hidden"
      },
      "item": {
        "borderBottom": "1px solid #f3f4f6"
      },
      "trigger": {
        "width": "100%",
        "padding": "1.5rem",
        "textAlign": "left",
        "backgroundColor": "transparent",
        "border": "none",
        "fontSize": "1.125rem",
        "fontWeight": "600",
        "color": "#111827",
        "transition": "background-color 0.2s ease",
        "hover": {
          "backgroundColor": "#f9fafb"
        }
      },
      "content": {
        "padding": "0 1.5rem 1.5rem",
        "color": "#6b7280",
        "lineHeight": "1.6"
      }
    }
  },
  "animations": {
    "fadeInUp": {
      "keyframes": {
        "from": {
          "opacity": "0",
          "transform": "translateY(30px)"
        },
        "to": {
          "opacity": "1",
          "transform": "translateY(0)"
        }
      },
      "duration": "0.6s",
      "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
    },
    "slideInLeft": {
      "keyframes": {
        "from": {
          "opacity": "0",
          "transform": "translateX(-50px)"
        },
        "to": {
          "opacity": "1",
          "transform": "translateX(0)"
        }
      },
      "duration": "0.8s",
      "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
    },
    "scaleIn": {
      "keyframes": {
        "from": {
          "opacity": "0",
          "transform": "scale(0.9)"
        },
        "to": {
          "opacity": "1",
          "transform": "scale(1)"
        }
      },
      "duration": "0.4s",
      "easing": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
    }
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  },
  "zIndex": {
    "hide": "-1",
    "auto": "auto",
    "base": "0",
    "docked": "10",
    "dropdown": "1000",
    "sticky": "1100",
    "banner": "1200",
    "overlay": "1300",
    "modal": "1400",
    "popover": "1500",
    "skipLink": "1600",
    "toast": "1700",
    "tooltip": "1800"
  }
}